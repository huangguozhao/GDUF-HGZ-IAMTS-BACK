            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}"
            },
            "url": {
              "raw": "{{baseUrl}}/test-results/1/report",
              "host": ["{{baseUrl}}"],
              "path": ["test-results", "1", "report"]
            }
          }
        }
      ]
    },
