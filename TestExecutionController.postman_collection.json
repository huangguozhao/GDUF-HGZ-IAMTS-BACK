    {
      "name": "6. 测试结果查询",
      "item": [
        {
          "name": "获取测试结果列表（无参数）",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/test-results",
              "host": ["{{baseUrl}}"],
              "path": ["test-results"]
            }
          }
        },
        {
          "name": "获取测试结果列表（带分页）",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/test-results?page=1&page_size=10",
              "host": ["{{baseUrl}}"],
              "path": ["test-results"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "page_size",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "查询失败的测试结果",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/test-results?status=failed&page=1&page_size=20",
              "host": ["{{baseUrl}}"],
              "path": ["test-results"],
              "query": [
                {
                  "key": "status",
