{
  "info": {
    "name": "TestExecutionController API测试",
    "description": "测试执行控制器的所有接口测试集合（共29个接口）",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080/api",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "1. 测试用例执行",
      "item": [
        {
          "name": "执行单个测试用例",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"environment\": \"test\",\n  \"timeout\": 30000\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/test-cases/1/execute",
              "host": ["{{baseUrl}}"],
              "path": ["test-cases", "1", "execute"]
            }
          }
        },
        {
          "name": "异步执行测试用例",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"environment\": \"test\",\n  \"async\": true\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/test-cases/1/execute-async",
              "host": ["{{baseUrl}}"],
              "path": ["test-cases", "1", "execute-async"]
            }
          }
        },
        {
          "name": "查询任务状态",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/tasks/test-task-id-123/status",
              "host": ["{{baseUrl}}"],
              "path": ["tasks", "test-task-id-123", "status"]
            }
          }
        },
        {
          "name": "取消任务执行",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}"
            },
            "url": {
              "raw": "{{baseUrl}}/tasks/test-task-id-123/cancel",
              "host": ["{{baseUrl}}"],
              "path": ["tasks", "test-task-id-123", "cancel"]
            }
          }
        },
        {
          "name": "获取执行结果详情",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/test-results/1",
              "host": ["{{baseUrl}}"],
              "path": ["test-results", "1"]
